	d."PromedioTotal", 
	d."PromedioTotalLiteral",
	d."Significacion"
FROM "Defensa" d
INNER JOIN "Persona" p ON d."Persona"=p.id
INNER JOIN "Especialidad" e ON d."Especialidad"=e.id
INNER JOIN "UnidadAcademica" ua ON d."UnidadAcademica"=ua.id
WHERE d."id"= $P{Defensa} ;]]>
