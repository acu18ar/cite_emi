	<queryString>
		<![CDATA[SELECT 
	d."id", 
	ua."UnidadAcademica",  
	e."Especialidad",
	d."Gestion", 
	d."PeriodoGestion", 
	p."Persona",	
	p."CI",
	d."FechaHora",
	d."Titulo", 
	d."Resumen", 
	d."Sala", 
	d."PromedioDefensa",
